<h1>Listing dogs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Size</th>
    <th>Breed</th>
    <th>Gender</th>
    <th>Picture url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dogs.each do |dog| %>
  <tr>
    <td><%= dog.name %></td>
    <td><%= dog.size %></td>
    <td><%= dog.breed %></td>
    <td><%= dog.gender %></td>
    <td><%= dog.picture_url %></td>
    <td><%= link_to 'Show', user_dog_path %></td>
    <td><%= link_to 'Edit', edit_user_dog_path(@user, dog) %></td>
    <td><%= link_to 'Destroy', dog, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dog', new_user_dog_path(@user, @dog) %>
