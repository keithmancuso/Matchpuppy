<% if notice %>
<div class="alert alert-info">
<%= notice %>
</div>
<% end %>

<div class="page-header">
<h1>Admin Dogs</h1>
</div>

<table class="table-striped table table-condensed">
<thead>  
<tr>
    <th>Name</th>
    <th>Size</th>
	<th>Breed</th>
    <th>Gender</th>
    <th>Energy</th>
	<th></th>
    
  </tr>
</thead>
<tbody>
<% @dogs.each do |dog|%>

<tr>
    <td><%= dog.name %></td>
    <td><%= dog.size %></td>
	<td><%= dog.breed %></td>
    <td><%= dog.gender %></td>
	<td><%= dog.energy %></td>
	<td>
		<%= link_to "Delete", user_dog_path(dog.user, dog), :method=>'delete', :class=>"btn btn-danger"%>
		
		
	</td>
  </tr>
<% end %>
</table>